#: indico_payment_manual/placeholders.py
msgid "First name of the registrant"
msgstr "Nome di chi si registra"

#: indico_payment_manual/placeholders.py
msgid "Last name of the registrant"
msgstr "Cognome di chi si registra"

#: indico_payment_manual/placeholders.py
msgid "Email address of the registrant"
msgstr "Email di chi si registra"

#: indico_payment_manual/placeholders.py
msgid "The database ID of the registration"
msgstr "Database ID di chi si registra"

#: indico_payment_manual/placeholders.py
msgid "The ID of the registration form"
msgstr "ID del modulo di registrazione"

#: indico_payment_manual/placeholders.py
