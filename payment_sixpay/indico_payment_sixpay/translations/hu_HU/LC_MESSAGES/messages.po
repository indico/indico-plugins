msgid "First name"
msgstr "Keresztnév"

#: indico_payment_sixpay/templates/event_payment_form.html
msgid "Last name"
msgstr "Vezetéknév"

#: indico_payment_sixpay/templates/event_payment_form.html
msgid "Total amount"
msgstr "Teljes összeg"

#: indico_payment_sixpay/templates/event_payment_form.html
msgid "Pay Now"
msgstr "Fizetés most"

